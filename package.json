{
	"name": "obsidian-habit-dashboard",
	"version": "1.0.0",
	"description": "This is a sample plugin for Obsidian (https://obsidian.md)",
	"main": "main.js",
	"scripts": {
		"dev:esbuild": "node esbuild.config.mjs",
		"dev:css": "npx @tailwindcss/cli -i ./src/main.css -o ./styles.css --watch",
		"dev": "npm-run-all --parallel dev:*",
		"build:esbuild": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"build:css": "npx @tailwindcss/cli -i ./src/main.css -o ./styles.css --minify",
		"build": "npm-run-all --parallel build:*",
		"version": "node version-bump.mjs && git add manifest.json versions.json",
		"svelte-check": "svelte-check --tsconfig tsconfig.json",
		"test": "jest"
	},
	"keywords": [],
	"author": "",
	"license": "MIT",
	"devDependencies": {
		"@babel/core": "^7.26.10",
		"@babel/preset-env": "^7.26.9",
		"@babel/preset-typescript": "^7.27.0",
		"@tailwindcss/cli": "^4.0.9",
		"@types/jest": "^29.5.14",
		"@types/node": "^16.11.6",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"babel-jest": "^29.7.0",
		"builtin-modules": "3.3.0",
		"esbuild": "0.17.3",
		"esbuild-svelte": "^0.9.0",
		"flowbite": "^3.1.2",
		"flowbite-svelte": "^1.2.7",
		"jest": "^29.7.0",
		"jest-environment-jsdom": "^29.7.0",
		"moment": "^2.30.1",
		"npm-run-all": "^4.1.5",
		"obsidian": "latest",
		"svelte": "^5.20.4",
		"svelte-check": "^4.1.4",
		"svelte-preprocess": "^6.0.3",
		"tailwindcss": "^4.1.7",
		"ts-jest": "^29.3.2",
		"tslib": "2.4.0",
		"typescript": "~5.0.0"
	},
	"dependencies": {
		"lucide-svelte": "^0.476.0"
	},
